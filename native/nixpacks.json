{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "buildCommand": "yarn build:web",
    "watchPatterns": ["native/**"],
    "nixpacksPlan": {
      "providers": ["node"],
      "phases": {
        "install": {
          "dependsOn": ["setup"],
          "cmds": ["yarn install"]
        }
      }
    }
  }
}
